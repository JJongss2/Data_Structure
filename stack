class Stack:
    def __init__(self):
        self.items = []
        
    def isEmpty(self):
        return len(self.items)==0
        
    def size(self):
        return len(self.items)
        
    def push(self, e):
        self.items.append(e)
        
    def pop(self):
        if self.isEmpty():
            print("Stack is Empty")
        else:
            return self.items.pop()
            
    def peek(self):
        if self.isEmpty():
            print("Stack is Empty")
        else:
            return self.items[-1]
            
    def clear(self):
        self.items = []
